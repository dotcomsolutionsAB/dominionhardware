        function capitalizeWords(str) {
            // Split the string into words
            let words = str.split('_');
            
            // Capitalize the first letter of each word
            for (let i = 0; i < words.length; i++) {
                words[i] = words[i].charAt(0).toUpperCase() + words[i].slice(1);
            }
            
            // Join the words back into a single string
            return words.join(' ');
        }

        function getVariantPrice(){
            if($('#option-choice-form input[name=quantity]').val() > 0 && checkAddToCartValidity()){
                $.ajax({
                    type:"POST",
                    url: 'https://www.dominionhardware.com/product/variant-price',
                    data: $('#option-choice-form').serializeArray(),
                    success: function(data){

                        if(data.price != 0)
                        {
                            $('.product-gallery-thumb .carousel-box').each(function (i) {
                                if($(this).data('variation') && data.variation == $(this).data('variation')){
                                    $('.product-gallery-thumb').slick('slickGoTo', i);
                                }
                            })

                            var addons_html = "";

                            data.addons.forEach(addon => {

                            // Get an array of keys for the current addon
                            let keys = Object.keys(addon);
                            console.log(keys);
                            
                            // Iterate over each key
                            keys.forEach(key => {
                                // Check if the key is not 'name' and the corresponding value is not empty
                                if (key !== 'id' && key !== 'product_id' && key !== 'variation_id' && addon[key] !== "") {
                                    
                                    // Get the value of the current key
                                    let options = addon[key].split(', ');
                                    
                                    // Generate select options HTML
                                    addons_html += `<div class="row no-gutters mb-3">
                                                        <div class="col-sm-2">
                                                            <div class="text-secondary fs-14 fw-800 mt-2">`
                                                                + capitalizeWords(key) +
                                                            `</div>
                                                        </div>
                                                        <div class="col-sm-10">
                                                            <select class="form-control aiz-selectpicker">`;

                                    // Iterate over each option and add it to the HTML
                                    options.forEach(option => {
                                        addons_html += `<option>${option}</option>`;
                                    });

                                    // Close the HTML tags
                                    addons_html += `</select>
                                                    </div>
                                                </div>`;

                                    // Output the select options HTML
                                    console.log(addons_html);
                                }
                            });
                                
                                // Repeat the same for thread_size and nabl_calibration
                            });
                            $('#product-addons').html(addons_html);

                            var s1 = data.rawprice * 0.95;
                            var s2 = data.rawprice * 0.90;

                            var bulk_discount = "<table class='table table-striped table-bordered'>";
                            bulk_discount += "<tr><th>Qty</th><th>Price</th></tr>";
                            bulk_discount += "<tr><td>1-10</td><td>"+s1+"</td></tr>";
                            bulk_discount += "<tr><td>11-20</td><td>"+s2+"</td></tr>";
                            bulk_discount += "</table>";

                            $('#bulk-discount-table').html(bulk_discount);

                            $('#option-choice-form #chosen_price_div').removeClass('d-none');
                            $('#option-choice-form #chosen_price_div #chosen_price').html(data.price);
                            var placeholder = "( "+data.price_excluding+" + "+data.tax+" GST )";
                            $('#option-choice-form #chosen_price_div #price_excluding_tax').html(placeholder);
                            $('#available-quantity').html(data.quantity);
                            $('.input-number').prop('max', data.max_limit);

                            
                            
                            if(parseInt(data.in_stock) == 0 && data.digital  == 0){
                            $('.buy-now').addClass('d-none');
                            $('.add-to-cart').addClass('d-none');
                            $('.out-of-stock').removeClass('d-none');
                            $('.invalid').addClass('d-none');
                            $('.qty-div').removeClass('d-none');

                            


                            }
                            else{
                            $('.buy-now').removeClass('d-none');
                            $('.add-to-cart').removeClass('d-none');
                            $('.out-of-stock').addClass('d-none');
                            $('.invalid').addClass('d-none');
                            $('.qty-div').removeClass('d-none');

                            }
                        }else{
                            $('.qty-div').addClass('d-none');
                            $('#chosen_price_div ').addClass('d-none');
                            $('.buy-now').addClass('d-none');
                            $('.add-to-cart').addClass('d-none');
                            $('.invalid').removeClass('d-none');
                        }

                        AIZ.extra.plusMinus();
                    }
                });
            }
        }

        function checkAddToCartValidity(){
            var names = {};
            $('#option-choice-form input:radio').each(function() { // find unique names
                names[$(this).attr('name')] = true;
            });
            var count = 0;
            $.each(names, function() { // then count them
                count++;
            });

            if($('#option-choice-form input:radio:checked').length == count){
                return true;
            }

            return false;
        }

        function addToCart(){
            
            if(checkAddToCartValidity()) {
                $('#addToCart').modal();
                $('.c-preloader').show();
                $.ajax({
                    type:"POST",
                    url: 'https://www.dominionhardware.com/cart/addtocart',
                    data: $('#option-choice-form').serializeArray(),
                    success: function(data){
                       $('#addToCart-modal-body').html(null);
                       $('.c-preloader').hide();
                       $('#modal-size').removeClass('modal-lg');
                       $('#addToCart-modal-body').html(data.modal_view);
                       AIZ.extra.plusMinus();
                       AIZ.plugins.slickCarousel();
                       updateNavCart(data.nav_cart_view,data.cart_count);
                    }
                });
            }
            else{
                AIZ.plugins.notify('warning', "Please choose all the options");
            }
        }

        function buyNow(){
                        
            if(checkAddToCartValidity()) {
                $('#addToCart-modal-body').html(null);
                $('#addToCart').modal();
                $('.c-preloader').show();
                $.ajax({
                    type:"POST",
                    url: 'https://www.dominionhardware.com/cart/addtocart',
                    data: $('#option-choice-form').serializeArray(),
                    success: function(data){
                        if(data.status == 1){
                            $('#addToCart-modal-body').html(data.modal_view);
                            updateNavCart(data.nav_cart_view,data.cart_count);
                            window.location.replace("https://www.dominionhardware.com/cart");
                        }
                        else{
                            $('#addToCart-modal-body').html(null);
                            $('.c-preloader').hide();
                            $('#modal-size').removeClass('modal-lg');
                            $('#addToCart-modal-body').html(data.modal_view);
                        }
                    }
               });
            }
            else{
                AIZ.plugins.notify('warning', "Please choose all the options");
            }
        }

        function bid_single_modal(bid_product_id, min_bid_amount){
                            $('#login_modal').modal('show');
                    }
        
        function clickToSlide(btn,id){
            $('#'+id+' .aiz-carousel').find('.'+btn).trigger('click');
            $('#'+id+' .slide-arrow').removeClass('link-disable');
            var arrow = btn=='slick-prev' ? 'arrow-prev' : 'arrow-next';
            if ($('#'+id+' .aiz-carousel').find('.'+btn).hasClass('slick-disabled')) {
                $('#'+id).find('.'+arrow).addClass('link-disable');
            }
        }

        function goToView(params) {
            document.getElementById(params).scrollIntoView({behavior: "smooth", block: "center"});
        }

        function copyCouponCode(code){
            navigator.clipboard.writeText(code);
            AIZ.plugins.notify('success', "Coupon Code Copied");
        }

        $(document).ready(function(){
            $('.cart-animate').animate({margin : 0}, "slow");

            $({deg: 0}).animate({deg: 360}, {
                duration: 2000,
                step: function(now) {
                    $('.cart-rotate').css({
                        transform: 'rotate(' + now + 'deg)'
                    });
                }
            });

            setTimeout(function(){
                $('.cart-ok').css({ fill: '#d43533' });
            }, 2000);
            
        });
    </script>

    
    <script>
        var acc = document.getElementsByClassName("aiz-accordion-heading");
        var i;
        for (i = 0; i < acc.length; i++) {
            acc[i].addEventListener("click", function() {
                this.classList.toggle("active");
                var panel = this.nextElementSibling;
                if (panel.style.maxHeight) {
                    panel.style.maxHeight = null;
                } else {
                    panel.style.maxHeight = panel.scrollHeight + "px";
                }
            });
        }
    </script>

    <script>
        function showFloatingButtons() {
            document.querySelector('.floating-buttons-section').classList.toggle('show');;
        }
    </script>

    
